set terminal pngcairo size 1200,700 enhanced font 'Arial,12'
set output 'comparacion_log.png'

set title "Comparacion de metodos para C(n,k)"
set xlabel "n"
set ylabel "Tiempo de ejecucion (s)"
set grid
set key outside

set logscale y

plot "tiempos.txt" using 1:2 with linespoints title "Metodo 1", \
     "tiempos.txt" using 1:3 with linespoints title "Metodo 2", \
     "tiempos.txt" using 1:4 with linespoints title "Metodo 3", \
     "tiempos.txt" using 1:5 with linespoints title "Metodo 4", \
     "tiempos.txt" using 1:6 with linespoints title "Metodo 5", \
     "tiempos.txt" using 1:7 with linespoints title "Metodo 6", \
     "tiempos.txt" using 1:8 with linespoints title "Metodo 7", \
     "tiempos.txt" using 1:9 with linespoints title "Metodo 8"

